{
  "title": "Orlando Careers",
  "name": "orlando-careers",
  "description": "A dashboard showcasing career options and skills needed in the Greater Orlando area.",
  "version": "0.1.0",
  "private": true,
  "author": "@JovaniPink",
  "license": "MIT",
  "keywords": [
    "dashboard",
    "reactjs",
    "gatsbyjs",
    "tailwindcss",
    "twin-macro",
    "orlando-job-market-data"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "npm lint --fix",
      "npm format"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "axios": "^0.21.1",
    "d3": "^6.3.1",
    "framer-motion": "^2.9.5",
    "gatsby": "^2.31.1",
    "gatsby-image": "2.8.0",
    "gatsby-link": "2.8.0",
    "gatsby-plugin-emotion": "^5.0.0",
    "gatsby-plugin-facebook-pixel": "1.0.5",
    "gatsby-plugin-google-analytics": "2.7.0",
    "gatsby-plugin-manifest": "^2.3.1",
    "gatsby-plugin-netlify": "2.7.0",
    "gatsby-plugin-offline": "^3.1.0",
    "gatsby-plugin-react-helmet": "^3.2.0",
    "gatsby-plugin-robots-txt": "1.5.3",
    "gatsby-plugin-sass": "^2.8.0",
    "gatsby-plugin-sharp": "2.11.1",
    "gatsby-plugin-sitemap": "2.8.0",
    "gatsby-plugin-twitter": "2.7.0",
    "gatsby-source-filesystem": "2.6.0",
    "gatsby-transformer-csv": "2.7.0",
    "gatsby-transformer-json": "2.8.0",
    "gatsby-transformer-sharp": "2.7.0",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-icons": "^3.11.0",
    "tailwindcss": "^2.0.1",
    "twin.macro": "^2.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.4.1",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^25.1.4",
    "@types/node": "14.14.10",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-jest": "^25.2.3",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@emotion/babel-plugin-jsx-pragmatic": "^0.1.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.3",
    "jest-watch-typeahead": "^0.5.0",
    "lint-staged": "^10.0.10",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3",
    "ts-jest": "^25.5.1"
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">= 14.15.1"
  }
}
